<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.edu.utn.sanfrancisco.clima" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Clima UTN</name>
    <description>
			Clima de San Francisco, Córdoba, Argentina - Facultad Regional San Francisco
    </description>
    <author email="seu@sanfrancisco.utn.edu.ar" href="http://seu.sanfrancisco.utn.edu.ar/">
      Secretaría de Extensión Universitaria
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait" />
</widget>
